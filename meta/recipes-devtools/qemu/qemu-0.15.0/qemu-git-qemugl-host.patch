+            if(mesa_lookup != gl_lookup)
+            {
+              fprintf(stderr, "wrong : %s %d %d !\n", name, mesa_lookup, gl_lookup);
+              exit(-1);
+            }
+          }*/
+          if (gl_lookup == -1)
