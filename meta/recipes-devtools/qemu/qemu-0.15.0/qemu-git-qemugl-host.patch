+static unsigned int calc_checksum(const void *_ptr, int length,
+                                  unsigned int seed)
+{
+    int i;
+    unsigned int crc = seed;
+    unsigned char *ptr = (unsigned char *) _ptr;
+
+    if (ptr == NULL)
+        return -1;
+    for (i = 0; i < length; i++) {
+        crc = crctable[(crc ^ *ptr++) & 0xFF] ^ (crc >> 8);
+    }
+    return crc;
