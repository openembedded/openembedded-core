+            fprintf(stderr, "not found in includes : %s\n", name);
+            //fprintf(outf, "/*  { %d, unknown value, \"%s\" },*/\n", count, name);
+          }
+          else
+          {
+            //fprintf(outf, "  { %d, 0x%04x, \"%s\" },\n", count, gl_lookup, name);
+            tokens[ntoken].value = gl_lookup;
