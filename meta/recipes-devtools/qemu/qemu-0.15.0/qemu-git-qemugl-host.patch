+static int opengl_chr_write(CharDriverState *chr, const uint8_t *buf, int len)
+{
+    uint64_t ptr = 0;
+    int i;
+
+    if (memchr(buf, 'x', len))
+        opengl_process_enable();
+    return len;
+    memcpy(buffer + buffer_len, buf, len);
+    buffer_len += len;
+
+    if (buffer_len >= 16) {
+        for (i = 0; i < 16; i ++)
+            ptr = (ptr << 4) + hexdigit[buffer[i]];
+
+        buffer_len -= 16;
+        if (buffer_len)
+            memcpy(buffer, buffer + 16, buffer_len);
+
+        mem_opengl(ptr);
+    }
+
+    return len;
+}
+
