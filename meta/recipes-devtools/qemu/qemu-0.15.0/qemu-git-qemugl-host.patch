+{
+    CharDriverState *chr = qemu_mallocz(sizeof(CharDriverState));
+
+    chr->opaque = chr;
+    chr->chr_write = opengl_chr_write;
+
+    qemu_chr_generic_open(chr);
+
+    return chr;
+}
