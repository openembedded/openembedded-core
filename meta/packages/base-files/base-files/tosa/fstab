/dev/mtdblock2	/ 	jffs2	rw,noatime	1  1 
/dev/mtdblock3	/home 	jffs2	defaults,noatime	1  2
proc		/proc	proc	defaults	0  0
/dev/hda1	/media/cf	auto	defaults,sync,noauto	0  0

tmpfs		/var		tmpfs   defaults        0  0
