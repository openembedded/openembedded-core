+int W100GetRotation(W100CardInfo *w100c)
+{
+    graphic_ctrl_u gc;
+    int randr;
+
+    gc.val = MMIO_IN32(mmGRAPHIC_CTRL);
+
+    switch (gc.f.portrait_mode) {
+    case 0:
+        randr = RR_Rotate_0;
+        break;
+    case 1:
+        randr = RR_Rotate_90;
+        break;
