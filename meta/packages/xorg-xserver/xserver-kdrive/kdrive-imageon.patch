+                  CARD16 x, CARD16 y, CARD16 w, CARD16 h)
+{
+    CARD16 ty;
+    switch (w100c->ctx.xform.randr & RR_Rotate_All) {
+    case RR_Rotate_0:
+        ty = y;
+        break;
+    case RR_Rotate_90:
+        if (w100c->ctx.xform.mirror) {
+            ty = y + w - 1;
+        } else {
+            ty = y;
+        }
+        break;
+    case RR_Rotate_180:
+        ty = y + h - 1;
+        break;
+    case RR_Rotate_270:
+    default:
+        if (w100c->ctx.xform.mirror) {
+            ty = y;
+        } else {
