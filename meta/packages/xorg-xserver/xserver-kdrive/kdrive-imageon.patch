+CARD16 W100XformX(W100CardInfo *w100c,
+                  CARD16 x, CARD16 y, CARD16 w, CARD16 h)
+{
+    CARD16 tx;
+    switch (w100c->ctx.xform.randr & RR_Rotate_All) {
+    case RR_Rotate_0:
+        if (w100c->ctx.xform.mirror) {
+            tx = x + w - 1;
+        } else {
+            tx = x;
+        }
+        break;
+    case RR_Rotate_90:
+        tx = x + h - 1;
+        break;
+    case RR_Rotate_180:
+        if (w100c->ctx.xform.mirror) {
+            tx = x;
+        } else {
+            tx = x + w - 1;
+        }
+        break;
+    case RR_Rotate_270:
+    default:
+        tx = x;
+        break;
+    }
+    return tx;
+}
+
