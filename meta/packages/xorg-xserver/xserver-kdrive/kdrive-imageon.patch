+int W100MapToHWX(W100CardInfo *w100c,
+                 int x, int y, int w, int h)
+{
+    int tx;
+    switch (w100c->hw_window.randr & RR_Rotate_All) {
+    case RR_Rotate_0:
+        tx = x;
+        break;
+    case RR_Rotate_90:
+        tx = w100c->hw_window.height - (y + h);
+        break;
+    case RR_Rotate_180:
+        tx = w100c->hw_window.width - (x + w);
+        break;
+    case RR_Rotate_270:
+        tx = y;
+        break;
+    default:
+        tx = x;
+    }
+    return tx;
+}
