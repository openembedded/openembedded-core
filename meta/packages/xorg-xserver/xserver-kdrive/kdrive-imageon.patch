+    case RR_Rotate_0:
+        ty = y;
+        break;
+    case RR_Rotate_90:
+        ty = x;
+        break;
+    case RR_Rotate_180:
+        ty = w100c->hw_window.height - (y + h);
+        break;
+    case RR_Rotate_270:
+        ty = w100c->hw_window.width - (x + w);
+        break;
+    default:
+        ty = y;
+    }
