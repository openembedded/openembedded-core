+clean:
+	dh_testdir
+	dh_testroot
+	-rm -f build
+	perl util/perlpath.pl /usr/bin
+	./Configure $(CONFARGS) debian-$(DEB_HOST_ARCH)
+	-make -f Makefile  clean clean-shared
