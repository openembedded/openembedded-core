+static void iegd_plb_tlbflush(struct agp_memory *mem)
+{
+	u32 sgx_mmu;
+
+	/* Flush TLB */
+	sgx_mmu = readl(private_data.registers + 0x40C00);
+	sgx_mmu &= 0xFFFFFFE0;
+	sgx_mmu |= 0x0C;
+	writel(sgx_mmu, private_data.registers + 0x40C00);
+
+	wmb();
+	sgx_mmu = readl(private_data.registers + 0x40C00);
+	sgx_mmu &= 0xFFFFFFE0;
+	writel(sgx_mmu, private_data.registers + 0x40C00);
+
+	return;
+}
